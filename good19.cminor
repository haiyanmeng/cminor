//fib

fib: function integer ( x: integer ) =
{
    if((x<1)) {
        return 0;
    } else {
        if((x<2)) {
            return 1;
        } else {
            return (fib((x-1))+fib((x-2)));
        }
    }
}
main: function integer () =
{
	//0 1 1 2 3 
    print fib(0),'\n';
    print fib(1),'\n';
    print fib(2),'\n';
    print fib(3),'\n';
    print fib(4),'\n';
    print fib(5),'\n';
    print fib(6),'\n';
    print fib(7),'\n';
    print fib(8),'\n';
    print fib(9),'\n';
    print fib(10),'\n';
    return 0;
}
