main: function integer (x:char,y:boolean) = {
	f(a*b,a + 1,b,'\n', '\0', '\r', '\t', '\\', '\b', c);
	f(a*b,a + 1,b,'\b',c);
	f(a+b,a + 1,b,'d',c);
	f(a+b,a + 1,b,"abcd\c","abc\n\0\r\t\\ \b", c);
	abc;
	(a);
	a+b;
	a+ 1;
	x:string = "abc"; // declaration string with initialization
	a: array[5] integer={1,2,3,4,5}; // declaration 1-D array with initialization
}

